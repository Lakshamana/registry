{
  "type": "commonjs",
  "scripts": {
    "lint": "eslint .",
    "lintstaged": "npm run lint -- --fix",
    "prepare": "husky install",
    "prepare:permission": "chmod ug+x .husky/*",
    "test": "jest --passWithNoTests",
    "test:watch": "npm test -- --watch",
    "test:ci": "npm test -- --coverage",
    "test:staged": "npm test -- --findRelatedTests",
    "dev": "ts-node -r dotenv/config src/mocks/index.ts",
    "debug": "NODE_OPTIONS=\"--inspect-brk\" ts-node -r dotenv/config src/mocks/index.ts",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig-build.json",
    "pack": "npm run build && npm pack",
    "start": "node dist/main/index"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.29",
    "dotenv": "^16.4.5",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "eslint": "^8.0.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "git-commit-msg-linter": "^4.1.2",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "module-alias": "^2.2.3",
    "rimraf": "^5.0.5",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^5.5.3"
  },
  "name": "@lakshamana-pocs/registry",
  "description": "",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": [],
